using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class SoundEffects : MonoBehaviour
{
    public AudioSource Running;
    public AudioSource Jumping;


    public void PlayRunning()
    {
        Running.Play();
    }
    public void PlayJumping()
    {
        Jumping.Play();
    }

    void OnTriggerEnter()
    {
        
    }
}

